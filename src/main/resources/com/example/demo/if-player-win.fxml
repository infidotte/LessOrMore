<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane fx:id="scenePaneWin" prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.IfPlayerWin">
   <children>
      <Label fx:id="text" layoutX="95.0" layoutY="14.0" prefHeight="93.0" prefWidth="106.0" text="Congratulations, you win!" textAlignment="CENTER" textOverrun="CLIP" wrapText="true" />
      <Button fx:id="newGame" layoutX="111.0" layoutY="137.0" mnemonicParsing="false" onAction="#onNewGameButton" text="New Game" />
      <Button fx:id="exit" layoutX="131.0" layoutY="260.0" mnemonicParsing="false" onAction="#onExitButton" text="Exit" />
   </children>
</AnchorPane>

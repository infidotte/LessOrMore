<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="scenePaneLier" prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.IfPlayerLie">
   <children>
      <Label fx:id="text" layoutX="108.0" layoutY="14.0" prefHeight="42.0" prefWidth="83.0" text="You're lose the game!" textAlignment="CENTER" textOverrun="CLIP" wrapText="true" />
      <Button fx:id="exit" layoutX="132.0" layoutY="260.0" mnemonicParsing="false" onAction="#onExitButton" text="Exit" />
      <Button fx:id="newGame" layoutX="113.0" layoutY="150.0" mnemonicParsing="false" onAction="#onNewGameButton" text="New game" />
   </children>
</AnchorPane>
